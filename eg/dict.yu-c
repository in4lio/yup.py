($title)
($set greeting "Hello world!\n")

($set name    ( Pi      ch      Zu          ))
($set type    ( double  char    float       ))
($set default ( { pi }  '~'     { 355/113 } ))
($set format  ( "%f"    "%c"    "%f"        ))

($set each ($range ($len name)))

#include <stdio.h>

int main(void)
{
	/* declare dict */
	($each \i.]
		($i type) ($i name) = ($i default);

	[ )
	printf(($greeting));

	/* print dict */
	($each \i.]
		printf(($i format), ($i name));

	[ )
	return 0;
}
